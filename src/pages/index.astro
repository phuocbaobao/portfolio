---
import BaseLayout from "../layouts/BaseLayout.astro";
---

<BaseLayout title="Home - Fullstack Developer Portfolio">
  <!-- Hero Section -->
  <section class="hero">
    <div class="container">
      <div class="hero-content animate-fade-in">
        <h1 class="hero-title animate-slide-up">
          Hi, I'm Bao â€” a
          <span class="text-gradient">Fullstack Software Engineer</span>
        </h1>
        <p class="hero-description animate-slide-up stagger-1">
          I create modern, responsive, and user-friendly web applications using
          the latest technologies like Python, JavaScript, Vue.js, and more.
        </p>
        <div class="hero-buttons animate-slide-up stagger-2">
          <a href="/contact" class="btn btn-primary">Contact Me</a>
          <a href="/blog" class="btn btn-outline">View Blog</a>
        </div>
      </div>
    </div>

    <!-- Animated Background -->
    <div class="hero-bg">
      <div class="gradient-orb orb-1"></div>
      <div class="gradient-orb orb-2"></div>
      <div class="gradient-orb orb-3"></div>
    </div>
  </section>

